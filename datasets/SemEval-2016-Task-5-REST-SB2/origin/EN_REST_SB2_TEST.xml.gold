            <Opinion category="SERVICE#GENERAL" polarity="negative"  />
            <Opinion category="LOCATION#GENERAL" polarity="positive"  />
            <Opinion category="RESTAURANT#GENERAL" polarity="negative"  />
        </Opinions>
